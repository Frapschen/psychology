<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdu.psychology.dao.CommonDao">
    <insert id="setCode" parameterType="java.lang.String">
        insert into code (code) values (#{id});
    </insert>
    <select id="getCode" parameterType="java.lang.String" resultType="com.cdu.psychology.entity.Code">
        select * from code where code=#{code} and used=0
    </select>
    <update id="upateCode" parameterType="java.lang.String">
        update code set used =1 where code=#{code}
    </update>

</mapper>